# AgentForge Environment Configuration
# Copy this file to .env and fill in your values

# ============================================
# CLINE CLI (Required for Infinity Build Award)
# ============================================
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxx

# ============================================
# KESTRA (Required for Wakanda Data Award)
# ============================================
KESTRA_API_URL=http://localhost:8080
KESTRA_API_KEY=your_kestra_api_key_here

# ============================================
# OUMI (Required for Iron Intelligence Award)
# ============================================
HUGGINGFACE_TOKEN=hf_xxxxxxxxxxxxxxxxxxxxx
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxx

# ============================================
# GITHUB (Required for all integrations)
# ============================================
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxx
GITHUB_REPO=yourusername/agentforge
GITHUB_OWNER=yourusername

# ============================================
# CODERABBIT (Required for Captain Code Award)
# ============================================
CODERABBIT_API_KEY=your_coderabbit_api_key

# ============================================
# VERCEL (Required for Stormbreaker Award)
# ============================================
VERCEL_TOKEN=your_vercel_token_here
VERCEL_PROJECT_ID=prj_xxxxxxxxxxxxx
VERCEL_DEPLOY_HOOK=https://api.vercel.com/v1/integrations/deploy/xxxxx

# ============================================
# DATABASE
# ============================================
DATABASE_URL=postgresql://kestra:kestra_password@localhost:5432/kestra
REDIS_URL=redis://localhost:6379

# ============================================
# CONFIGURATION
# ============================================
NODE_ENV=development
PORT=3000

MIN_TEST_COVERAGE=80
MIN_REVIEW_SCORE=8.0
DEPLOYMENT_CONFIDENCE_THRESHOLD=0.85
